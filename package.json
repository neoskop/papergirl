{
  "name": "papergirl",
  "version": "3.2.3",
  "description": "",
  "author": "Arne Diekmann <diekmann@neoskop.de>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node --loader ts-node/esm -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon -e ts -w \"src/**/*\" --exec \"node --loader ts-node/esm -r tsconfig-paths/register ./src/main.ts\"",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.421.0",
    "@godaddy/terminus": "4.12.1",
    "@kubernetes/client-node": "0.19.0",
    "@nestjs/common": "10.2.6",
    "@nestjs/core": "10.2.6",
    "@nestjs/event-emitter": "2.0.2",
    "@nestjs/platform-express": "10.2.6",
    "@nestjs/terminus": "10.1.1",
    "@slack/webhook": "6.1.0",
    "async": "3.2.4",
    "axios": "1.5.1",
    "chalk": "5.3.0",
    "chokidar": "3.5.3",
    "deepmerge": "4.3.1",
    "dotenv": "16.3.1",
    "joi": "17.10.2",
    "js-yaml": "4.1.0",
    "minipass": "7.0.4",
    "nats": "2.16.0",
    "nest-winston": "1.9.4",
    "openid-client": "5.5.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "strip-ansi": "7.1.0",
    "ts-interface-checker": "1.0.2",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.18",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.2.6",
    "@types/async": "3.2.21",
    "@types/express": "4.17.18",
    "@types/jest": "29.5.5",
    "@types/js-yaml": "4.0.6",
    "@types/node": "18.18.1",
    "@types/supertest": "2.0.13",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.7.0",
    "nodemon": "3.0.1",
    "prettier": "3.0.3",
    "supertest": "6.3.3",
    "ts-interface-builder": "0.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
