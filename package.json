{
  "name": "papergirl",
  "version": "3.2.6",
  "description": "",
  "author": "Arne Diekmann <diekmann@neoskop.de>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node --loader ts-node/esm -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon -e ts -w \"src/**/*\" --exec \"node --loader ts-node/esm -r tsconfig-paths/register ./src/main.ts\"",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.679.0",
    "@godaddy/terminus": "4.12.1",
    "@kubernetes/client-node": "0.19.0",
    "@nestjs/common": "10.4.16",
    "@nestjs/core": "10.4.6",
    "@nestjs/event-emitter": "2.1.1",
    "@nestjs/platform-express": "10.4.6",
    "@nestjs/terminus": "10.2.3",
    "@slack/webhook": "7.0.6",
    "async": "3.2.6",
    "axios": "1.8.2",
    "chalk": "5.3.0",
    "chokidar": "3.6.0",
    "deepmerge": "4.3.1",
    "dotenv": "16.4.5",
    "joi": "17.13.3",
    "js-yaml": "4.1.0",
    "minipass": "7.1.2",
    "nats": "2.28.2",
    "nest-winston": "1.10.0",
    "openid-client": "5.7.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "strip-ansi": "7.1.0",
    "ts-interface-checker": "1.0.2",
    "winston": "3.15.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.5",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.6",
    "@types/async": "3.2.25",
    "@types/express": "4.17.23",
    "@types/jest": "29.5.14",
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.18.0",
    "@types/supertest": "2.0.16",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "nodemon": "3.1.10",
    "prettier": "3.3.3",
    "supertest": "6.3.3",
    "ts-interface-builder": "0.3.3",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
