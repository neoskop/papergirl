{
  "name": "papergirl",
  "version": "3.2.0",
  "description": "",
  "author": "Arne Diekmann <diekmann@neoskop.de>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon -e ts -w \"src/**/*\" --exec \"node --experimental-specifier-resolution=node --loader ts-node/esm -r tsconfig-paths/register ./src/main.ts\"",
    "start:prod": "node --experimental-specifier-resolution=node dist/main.js",
    "start:hmr": "node --experimental-specifier-resolution=node dist/server",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.216.0",
    "@godaddy/terminus": "4.11.2",
    "@kubernetes/client-node": "0.17.1",
    "@nestjs/common": "9.2.0",
    "@nestjs/core": "9.2.0",
    "@nestjs/event-emitter": "1.3.1",
    "@nestjs/platform-express": "9.2.0",
    "@nestjs/terminus": "9.1.4",
    "@slack/webhook": "6.1.0",
    "async": "3.2.4",
    "axios": "1.2.0",
    "chalk": "5.1.2",
    "chokidar": "3.5.3",
    "deepmerge": "4.2.2",
    "dotenv": "16.0.3",
    "joi": "17.7.0",
    "js-yaml": "4.1.0",
    "minipass": "3.3.5",
    "nats": "2.9.1",
    "nest-winston": "1.8.0",
    "openid-client": "5.3.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.7",
    "strip-ansi": "7.0.1",
    "ts-interface-checker": "1.0.2",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.5",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.2.0",
    "@types/async": "3.2.15",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.3",
    "@types/js-yaml": "4.0.5",
    "@types/node": "18.11.9",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.3.1",
    "nodemon": "2.0.20",
    "prettier": "2.8.0",
    "supertest": "6.3.1",
    "ts-interface-builder": "0.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
